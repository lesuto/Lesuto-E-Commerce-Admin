{
  "name": "Lesuto-E-Commerce-Admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev:dashboard": "vite --clearScreen false",
    "dev": "concurrently --kill-others npm:dev:*",
    "build": "tsc",
    "build:dashboard": "vite build",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@vendure/asset-server-plugin": "3.5.2",
    "@vendure/core": "3.5.2",
    "@vendure/dashboard": "3.5.2",
    "@vendure/email-plugin": "3.5.2",
    "@vendure/graphiql-plugin": "3.5.2",
    "dotenv": "17.2.3",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.3",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@vendure/cli": "3.5.2",
    "concurrently": "9.2.1",
    "typescript": "5.8.2"
  }
}
